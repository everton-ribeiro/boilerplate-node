{
  "name": "boilerplate-node",
  "version": "1.0.0",
  "description": "Boilerplate para criação de projetos back-end.",
  "main": "src/index.ts",
  "scripts": {
    "start": "node build/index.js",
    "dev": "tsx watch -r tsconfig-paths/register src/index.ts",
    "build": "tsup src",
    "clean": "del-cli --force ./build",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/everton-ribeiro/boilerplate-node.git"
  },
  "keywords": [],
  "author": "Everton Ribeiro",
  "license": "MIT",
  "dependencies": {
    "fastify": "5.6.1"
  },
  "devDependencies": {
    "del-cli": "7.0.0",
    "terser": "5.44.0",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.5.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  }
}
