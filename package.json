{
  "name": "boilerplate-node",
  "version": "1.0.0",
  "description": "Boilerplate para criação de projetos back-end.",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "husky",
    "commit": "git-cz",
    "start": "node build/index.js",
    "dev": "tsx watch -r tsconfig-paths/register src/index.ts",
    "build": "tsup src",
    "clean": "del-cli --force ./build",
    "lint": "eslint .",
    "lint:fix": "eslint  --fix .",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:clean": "del-cli --force ./coverage",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/everton-ribeiro/boilerplate-node.git"
  },
  "keywords": [],
  "author": "Everton Ribeiro",
  "license": "MIT",
  "dependencies": {
    "fastify": "5.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@eslint/compat": "1.4.0",
    "@eslint/js": "9.37.0",
    "@types/node": "24.7.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "commitizen": "4.3.1",
    "conventional-changelog-atom": "5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "7.0.0",
    "eslint": "9.37.0",
    "eslint-config-airbnb-extended": "2.3.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "init": "0.1.2",
    "prettier": "3.6.2",
    "terser": "5.44.0",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.5.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
